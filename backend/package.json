{
  "name": "algohire-webhook-backend",
  "version": "1.0.0",
  "description": "Reliable webhook relay system for Algohire platform - handles event distribution to external systems with guaranteed delivery",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "webhook-worker": "node src/workers/webhookWorker.js",
    "event-worker": "node src/workers/eventProcessor.js",
    "workers": "concurrently \"npm run webhook-worker\" \"npm run event-worker\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run workers\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "webhook",
    "event-system",
    "message-queue",
    "mongodb",
    "redis",
    "bullmq"
  ],
  "author": "Algohire Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "mongoose": "^8.19.1",
    "redis": "^5.8.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
